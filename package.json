{
  "name": "wild-graphql-datasource",
  "version": "1.5.0",
  "description": "Grafana data source to interpret GraphQL query results as timeseries data",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix && prettier --write --list-different .",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Lavender Shannon",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "10.4.1",
    "@grafana/e2e-selectors": "10.4.1",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.72.0",
    "sass-loader": "13.3.3",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "imports-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^46.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "^12.0.2",
    "@grafana/runtime": "^12.0.2",
    "@grafana/schema": "^12.0.2",
    "@grafana/ui": "^12.0.2",
    "graphiql": "^3.1.2",
    "graphql": "^16.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "2.8.1"
  },
  "packageManager": "npm@8.19.4"
}
